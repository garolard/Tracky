<Application
    x:Class="Tracky.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Tracky"
    xmlns:vm="using:Tracky.ViewModels"
    RequestedTheme="Light">

    <Application.Resources>
        <vm:MainViewModel x:Key="Main"/>
        <vm:DetailViewModel x:Key="Detail"/>
        <local:RatingsConverter x:Key="Ratings"/>
        <local:VotesConverter x:Key="Votes"/>
    </Application.Resources>
</Application>
